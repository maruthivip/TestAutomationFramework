# Environment Configuration
TEST_ENV=qa
NODE_ENV=test

# Application URLs
BASE_URL=https://qa.uhc-gps.com
API_BASE_URL=https://api-qa.uhc-gps.com
SOAP_BASE_URL=https://soap-qa.uhc-gps.com

# Authentication
OAUTH2_CLIENT_ID=your_client_id
OAUTH2_CLIENT_SECRET=your_client_secret
OAUTH2_TOKEN_URL=https://auth.uhc-gps.com/oauth2/token
OAUTH2_SCOPE=read write

# Test User Credentials
MEMBER_USERNAME=test.member@uhc.com
MEMBER_PASSWORD=TestPassword123!

EMPLOYEE_USERNAME=test.employee@uhc.com
EMPLOYEE_PASSWORD=TestPassword123!

ADMIN_USERNAME=test.admin@uhc.com
ADMIN_PASSWORD=TestPassword123!

BROKER_USERNAME=test.broker@uhc.com
BROKER_PASSWORD=TestPassword123!

CLAIMS_PROCESSOR_USERNAME=test.claims@uhc.com
CLAIMS_PROCESSOR_PASSWORD=TestPassword123!

CUSTOMER_SERVICE_USERNAME=test.cs@uhc.com
CUSTOMER_SERVICE_PASSWORD=TestPassword123!

PROVIDER_USERNAME=test.provider@uhc.com
PROVIDER_PASSWORD=TestPassword123!

FINANCE_USERNAME=test.finance@uhc.com
FINANCE_PASSWORD=TestPassword123!

COMPLIANCE_USERNAME=test.compliance@uhc.com
COMPLIANCE_PASSWORD=TestPassword123!

# Database Configuration
COSMOS_DB_ENDPOINT=https://uhc-gps-qa.documents.azure.com:443/
COSMOS_DB_KEY=your_cosmos_db_key
COSMOS_DB_DATABASE=uhc_gps_qa
COSMOS_DB_CONTAINER=test_data

ORACLE_DB_HOST=oracle-qa.uhc.com
ORACLE_DB_PORT=1521
ORACLE_DB_SERVICE=UHCGPS
ORACLE_DB_USERNAME=test_user
ORACLE_DB_PASSWORD=test_password

# Test Configuration
PARALLEL_WORKERS=4
TEST_TIMEOUT=120000
ACTION_TIMEOUT=30000
NAVIGATION_TIMEOUT=60000

# Reporting
ALLURE_RESULTS_DIR=allure-results
HTML_REPORT_DIR=test-results/html-report
SCREENSHOT_ON_FAILURE=true
VIDEO_ON_FAILURE=true
TRACE_ON_FAILURE=true

# CI/CD Configuration
CI=false
HEADLESS=true
START_LOCAL_SERVER=false

# Security & Compliance
ENABLE_SECURITY_TESTS=true
ENABLE_ACCESSIBILITY_TESTS=true
ENABLE_PERFORMANCE_TESTS=true
HIPAA_COMPLIANCE_MODE=true

# Logging
LOG_LEVEL=info
LOG_FILE=logs/test-execution.log

# Network Configuration
IGNORE_HTTPS_ERRORS=true
BYPASS_CSP=false
EXTRA_HTTP_HEADERS={"Accept-Language":"en-US,en;q=0.9"}

# Mobile Testing
MOBILE_DEVICE=Pixel 5
TABLET_DEVICE=iPad Pro

# Performance Thresholds
PERFORMANCE_BUDGET_LOAD_TIME=3000
PERFORMANCE_BUDGET_FCP=1500
PERFORMANCE_BUDGET_LCP=2500