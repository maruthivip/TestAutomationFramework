{
  "name": "automationframework",
  "version": "1.0.0",
  "description": "Production-grade AutomationFramework for web, API, and database testing.",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:api": "playwright test tests/api/",
    "test:e2e": "playwright test tests/e2e/",
    "test:smoke": "playwright test --grep @smoke",
    "test:regression": "playwright test --grep @regression",
    "test:parallel": "playwright test --workers=4",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit",
    "test:mobile": "playwright test --project=mobile-chrome",
    "report": "playwright show-report",
    "report:allure": "allure serve allure-results",
    "report:generate": "allure generate allure-results --clean",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "install:browsers": "playwright install",
    "install:deps": "playwright install-deps",
    "coverage": "nyc playwright test",
    "coverage:report": "nyc report --reporter=html",
    "clean": "rimraf test-results allure-results coverage .nyc_output",
    "setup": "npm run install:browsers && npm run install:deps",
    "ci:test": "npm run lint && npm run type-check && npm run test",
    "dev": "npm run test:ui",
    "bdd": "cucumber-js --require-module ts-node/register --require features/step-definitions/**/*.ts --require features/support/world.ts --require features/support/hooks.ts features/**/*.feature",
    "prepare": "husky install",
    "report:cucumber": "cucumber-js --require-module ts-node/register --require features/step-definitions/**/*.ts --require features/support/world.ts --require features/support/hooks.ts features/**/*.feature --format html:cucumber-report.html"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "playwright",
    "typescript",
    "automation",
    "testing",
    "healthcare",
    "e2e",
    "api"
  ],
  "author": "Healthcare QA Team",
  "license": "MIT",
  "devDependencies": {
    "@azure/cosmos": "^4.5.0",
    "@cucumber/cucumber": "^12.1.0",
    "@playwright/test": "^1.54.2",
    "@types/node": "^20.8.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "allure-commandline": "^2.24.0",
    "allure-cucumberjs": "^3.3.2",
    "allure-playwright": "^2.15.1",
    "axios": "^1.11.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-playwright": "^0.16.0",
    "eslint-plugin-prettier": "^5.0.0",
    "faker": "^5.5.3",
    "js-yaml": "^4.1.0",
    "moment": "^2.30.1",
    "nyc": "^15.1.0",
    "playwright": "^1.54.2",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "soap": "^1.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "xml2js": "^0.6.2"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "oracledb": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
